{
	"schemaVersion": 1,
	"id": "qga-utilities",
	"version": "${version}",
	"name": "QGA Utilities",
	"description": "Utilities mod for QQue's Great Adventure. A part of the Fabric switch.",
	"authors": [
		"QQueTV",
		"wrldmap"
	],
	"contact": {
		"sources": "https://github.com/wrldmap/QGA_Utilities"
	},
